    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>TESTESTES</title>
    
    <link rel="icon" type="image/x-icon" href="~/asset/favicon.ico">

    <!--Bootstrap & FontAwesome-->
    <link type="text/css" href="/fontawesome-free/css/all.min.css" rel="stylesheet" />
    <link type="text/css" href="/fontawesome-free/css/fontawesome.min.css" rel="stylesheet" />
    <link type="text/css" href="/fontawesome-free/css/solid.min.css" rel="stylesheet" />
    <link type="text/css" href="/css/bootstrap.min.css" rel="stylesheet" />

    <!--Important Addition-->
    <link type="text/css" href="/sweetalert2.min.css" rel="stylesheet" />
    <link type="text/css" href="/css/jquery.dataTables.min.css" rel="stylesheet" />
    <link type="text/css" href="/css/select2.min.css" rel="stylesheet" />

