    <script type="text/javascript" src="/modules/accessibility.js"></script>
    <script type="text/javascript" src="/sweetalert2.all.min.js"></script>
    <script type="text/javascript" src="/js/select2.min.js"></script>
    <script type="text/javascript" src="/fontawesome-free/js/all.min.js"></script>
    <script type="text/javascript" src="/fontawesome-free/js/fontawesome.min.js"></script>
    <script type="text/javascript" src="/fontawesome-free/js/solid.min.js"></script>
    <!--Custom Javascript-->
