</head>
<body>
    <div class="main-wrapper">
        <!-- ----NAVBAR---- -->
        <div class="header-container fixed-top">
            <header class="header navbar navbar-expand-sm expand-header">
                <div class="header-left d-flex">
                    <div class="logo">
                        DevEngine
                    </div>
                    <a href="#" class="sidebarCollapse" id="toggleSidebar" data-placement="bottom">
                        <span class="fas fa-bars"></span>
                    </a>
                </div>
                <ul class="navbar-item flex-row ml-auto">
                    <li class="nav-item dropdown user-profile-dropdown">
                        <a href="#" class="nav-link user" id="notifyDropdown" data-bs-toggle="dropdown">
                            <span class="fas fa-bell"></span>
                            <p class="count">5</p>
                        </a>
                        <ul class="dropdown-menu dropdown-menu-end animated-dropdown notifications-menu" aria-labelledby="notifyDropdown" data-bs-auto-close="false">

                            <!-- Users Notifications -->
                            <li class="dropdown-item dropdown-toggle d-flex align-items-center" data-bs-toggle="collapse" data-bs-target="#userNotifications">
                                <div class="col-2 d-flex align-items-center justify-content-center text-center">
                                    <span class="fas fa-user"></span>
                                </div>
                                <div class="col-8 d-flex align-items-center justify-content-center text-center">
                                    <span>Users &nbsp;</span>
                                    <span class="count badge bg-primary">3</span>
                                </div>
                            </li>
                            <ul id="userNotifications" class="collapse list-unstyled dropdown-submenu">
                                <li><hr class="dropdown-divider"></li>
                                <li class="dropdown-item"><span class="fas fa-user-plus"></span> <strong>Alex</strong> signed up</li>
                                <li class="dropdown-item"><span class="fas fa-comment"></span> <strong>Sarah</strong> commented on your post</li>
                                <li class="dropdown-item"><span class="fas fa-thumbs-up"></span> <strong>John</strong> liked your photo</li>
                                <li><hr class="dropdown-divider"></li>
                            </ul>
                    
                            <!-- Selling Notifications -->
                            <li class="dropdown-item dropdown-toggle d-flex align-items-center" data-bs-toggle="collapse" data-bs-target="#sellingNotifications">
                                <div class="col-2 d-flex align-items-center justify-content-center text-center">
                                    <span class="fas fa-shopping-cart"></span> 
                                </div>
                                <div class="col-8 d-flex align-items-center justify-content-center text-center">
                                    <span>Sellings &nbsp;</span>
                                    <span class="count badge bg-success">2</span>
                                </div>
                            </li>
                            <ul id="sellingNotifications" class="collapse list-unstyled dropdown-submenu">
                                <li><hr class="dropdown-divider"></li>
                                <li class="dropdown-item"><span class="fas fa-dollar-sign"></span> <strong>$1200</strong> sale completed</li>
                                <li class="dropdown-item"><span class="fas fa-box-open"></span> <strong>David</strong> purchased 3 items</li>
                                <li><hr class="dropdown-divider"></li>
                            </ul>
                    
                            <!-- New Products Notifications -->
                            <li class="dropdown-item dropdown-toggle d-flex align-items-center" data-bs-toggle="collapse" data-bs-target="#productNotifications">
                                <div class="col-2 d-flex align-items-center justify-content-center text-center">
                                    <span class="fas fa-box"></span>    
                                </div>
                                <div class="col-8 d-flex align-items-center justify-content-center text-center">
                                    <span>New Products &nbsp;</span>
                                    <span class="count badge bg-warning">2</span>
                                </div>
                            </li>
                            <ul id="productNotifications" class="collapse list-unstyled dropdown-submenu">
                                <li><hr class="dropdown-divider"></li>
                                <li class="dropdown-item"><span class="fas fa-tag"></span> <strong>New iPhone 15</strong> added to store</li>
                                <li class="dropdown-item"><span class="fas fa-laptop"></span> <strong>Gaming Laptop</strong> now available</li>
                            </ul>
                    
                            <!-- Divider -->
                            <li><hr class="dropdown-divider"></li>
                    
                            <!-- View All Notifications -->
                            <li><a class="dropdown-item text-center fw-bold" href="#" style="font-size: 12px;">View All Notifications . . .</a></li>
                        </ul>
                    </li>
                    
                    <li class="nav-item dropdown user-profile-dropdown">
                        <a href="#" class="nav-link user" id="userDropdown" data-bs-toggle="dropdown">
                            <span class="fas fa-user"></span>
                        </a>
                        <div class="dropdown-menu dropdown-menu-end animated-dropdown notifications-menu" aria-labelledby="userDropdown" data-bs-auto-close="false">
                            <div class="user-profile-section">
                                <div class="media mx-auto">
                                    <img src="/assets/user-icon-front-side-white-background.jpg" alt="Okay" class="img-fluid mr-2">
                                    <div class="media-body">
                                        <h5>Helal Uddin</h5>
                                        <p>Super Admin</p>
                                    </div>
                                </div>
                            </div>
                            <ul class="media-second-body">
                                <a class="dropdown-item d-flex align-items-center justify-content-between" href="">
                                    <div class="justify-content-start">
                                        <span class="fas fa-user"></span> 
                                    </div>
                                    <div class="align-items-center justify-content-center text-center">
                                        <span>Profile Setting &nbsp;</span>
                                    </div>
                                    <div></div>
                                </a>
                                <a class="dropdown-item d-flex align-items-center justify-content-between" href="">
                                    <div class="justify-content-start">
                                        <span class="fas fa-gear"></span> 
                                    </div>
                                    <div class="align-items-center justify-content-center text-center">
                                        <span>Account Setting &nbsp;</span>
                                    </div>
                                    <div></div>
                                </a>
                                <li><hr class="dropdown-divider"></li>
                                <a class="dropdown-item d-flex align-items-center justify-content-between" href="">
                                    <div class="justify-content-start">
                                        <span class="fas fa-sign-out-alt"></span> 
                                    </div>
                                    <div class="align-items-center justify-content-center text-center">
                                        <span>Logout &nbsp;</span>
                                    </div>
                                    <div></div>
                                </a>
                            </ul>
                        </div>
                    </li>
                </ul>
            </header>
        </div>
        <!-- ----NAVBAR END---- -->

        <!-- ----SIDEBAR---- -->
        <div class="left-menu">
            <div class="menubar-content">
                <nav class="animated bounceInDown">
                    <ul id="sidebar">
                        <li class="active">
                            <a href="#"><i class="fas fa-home"></i><span>Dashboard</span></a>
                        </li>
                        <li>
                            <a href="#"><i class="fas fa-chart-bar"></i><span>Analytics</span></a>
                        </li>
                        <li>
                            <a href="#"><i class="fas fa-user"></i><span>Profile</span></a>
                        </li>
                        <li>
                            <a href="#"><i class="fas fa-cog"></i><span>Settings</span></a>
                        </li>
                        <li>
                            <a href="#"><i class="fas fa-sign-out-alt"></i><span>Logout</span></a>
                        </li>
                        <li>
                            <a href="#"><i class="fas fa-chart-bar"></i><span>Analytics</span></a>
                        </li>
                        <li>
                            <a href="#"><i class="fas fa-user"></i><span>Profile</span></a>
                        </li>
                        <li>
                            <a href="#"><i class="fas fa-cog"></i><span>Settings</span></a>
                        </li>
                        <li>
                            <a href="#"><i class="fas fa-sign-out-alt"></i><span>Logout</span></a>
                        </li>
                    </ul>
                </nav>
            </div>
        </div>
        <!-- ----SIDEBAR END---- -->

        <!-- ----CONTENT---- -->
        <div class="content-wrapper">
            <%- body %>
        </div>
        <!-- ----CONTENT END---- -->
